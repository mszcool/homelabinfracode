<% unless @iptables.nil? || @iptables.empty? -%>
<% @iptables.each do |table, data| -%>
*<%= table %>
<% unless data['chains'].nil? || data['chains'].empty? -%>
<% data['chains'].each do |chain, value| -%>
:<%= chain %> <%= value %>
<% end -%>
<% end -%>
<% unless data['rules'].nil? || data['rules'].empty? -%>
<% data['rules'].each do |rule| -%>
<%= rule %>
<% end -%>
<% end -%>
COMMIT
<% end -%>
<% end -%>
